<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="Clefs" alignment="CENTER" prefHeight="485.0" prefWidth="707.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.apcles.ClefsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="39.0" prefWidth="666.0">
      <children>
         <Label prefHeight="17.0" prefWidth="199.0" text="Connecté en tant que:" />
         <Label prefHeight="17.0" prefWidth="389.0" text=" " />
         <Button fx:id="deconnexion" minWidth="66.0" mnemonicParsing="false" onAction="#deconnexion" prefHeight="25.0" prefWidth="123.0" text="Déconnexion" />
      </children>
   </HBox>
   <HBox prefHeight="43.0" prefWidth="667.0">
      <children>
         <TextField fx:id="searchBar" maxWidth="-Infinity" prefHeight="28.0" prefWidth="688.0" />
         <Button minWidth="66.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="237.0" text="Rechercher" />
      </children>
   </HBox>
   <ListView fx:id="listKey" prefHeight="200.0" prefWidth="200.0" />
   <HBox prefHeight="31.0" prefWidth="667.0">
      <children>
         <Label prefHeight="17.0" prefWidth="458.0" text=" " />
         <Button fx:id="supprimer" mnemonicParsing="false" onAction="#Supprimer" prefHeight="25.0" prefWidth="98.0" text="Supprimer" />
         <Label prefHeight="17.0" prefWidth="14.0" text=" " />
         <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="98.0" text="Modifier" />
         <Label prefHeight="17.0" prefWidth="14.0" text=" " />
         <Button fx:id="ajouter" mnemonicParsing="false" onAction="#goToAjouter" prefHeight="25.0" prefWidth="98.0" text="Ajouter" />
      </children></HBox>
</VBox>
